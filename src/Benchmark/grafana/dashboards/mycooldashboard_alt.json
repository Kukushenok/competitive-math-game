{
  "id": null,
  "uid": "http-monitoring-dashboard-v2",
  "title": "HTTP Service Monitoring (Fixed)",
  "tags": ["http", "performance", "k6", "dotnet"],
  "timezone": "browser",
  "schemaVersion": 35,
  "version": 1,
  "refresh": "5s",
  "time": {"from": "now-5m", "to": "now"},
  "templating": {"list": []},
  "panels": [
{
  "id": 1,
  "title": "CPU Utilization (Normalized)",
  "type": "timeseries",
  "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0},
  "targets": [
    {
      "expr": "system_runtime_cpu_usage",
      "legendFormat": "CPU Usage",
      "refId": "A"
    }
  ],
  "fieldConfig": {
    "defaults": {
      "unit": "percent",
      "color": {"mode": "palette-classic"},
      "thresholds": {
        "steps": [
          {"color": "green", "value": null},
          {"color": "yellow", "value": 70},
          {"color": "red", "value": 85}
        ]
      }
    }
  },
  "options": {
    "legend": {
      "showLegend": true,
      "displayMode": "list",
      "placement": "bottom"
    },
    "tooltip": {
      "mode": "single"
    }
  }
},
    {
  "id": 3,
  "title": "Threadpool Wait (Seconds)",
  "type": "timeseries",
  "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8},
  "targets": [
    {
      "expr": "rate(dotnet_threadpool_wait_seconds_sum[$__rate_interval])",
      "legendFormat": "Threadpool Wait (s)",
      "refId": "A"
    }
  ],
  "fieldConfig": {
    "defaults": {
      "unit": "s",
      "color": {"mode": "palette-classic"},
      "thresholds": {
        "steps": [
          {"color": "green", "value": null},
          {"color": "yellow", "value": 0.5},
          {"color": "red", "value": 1}
        ]
      }
    },
    "custom": {
      "gradientMode": "hue"
    }
  },
 
  "options": {
    "legend": {
      "showLegend": true,
      "displayMode": "list",
      "placement": "bottom"
    },
    "tooltip": {
      "mode": "single"
    }
  }
},
    {
  "id": 2,
  "title": "CPU Utilization Summary",
  "type": "table",
  "gridPos": {"h": 8, "w": 6, "x": 12, "y": 0},
  "targets": [
    {
      "expr": "system_runtime_cpu_usage",
      "legendFormat": "CPU Usage",
      "refId": "A"
    }
  ],
  "fieldConfig": {
    "defaults": {
      "unit": "percent",
      "color": {"mode": "palette-classic"},
      "custom": {
        "displayMode": "auto"
      }
    },
    "overrides": []
  },
  "options": {
    "showHeader": true
  },
  "transformations": [
    {
      "id": "reduce",
      "options": {
        "reducers": ["first", "last", "min", "max", "mean"],
        "labelsToFields": true
      }
    },
    {
      "id": "organize",
      "options": {
        "excludeByName": {
          "__name__": true,
          "instance": true,
          "job": true,
          "Field": true
        },
        "renameByName": {
          "First": "First Value",
          "Last": "Last Value",
          "Min": "Minimum",
          "Max": "Maximum",
          "Mean": "Average"
        }
      }
    },
    {
      "id": "transpose",
      "options": {}
    }
  ]
},
    {
      "id": 4,
      "title": "Memory Usage",
      "type": "stat",
      "gridPos": {"h": 8, "w": 6, "x": 12, "y": 8},
      "targets": [
        {
          "expr": "dotnet_total_memory_bytes",
          "legendFormat": "Memory",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "bytes",
          "color": {"mode": "palette-classic"},
          "thresholds": {
            "steps": [
              {"color": "green", "value": null},
              {"color": "yellow", "value": 500000000},
              {"color": "red", "value": 800000000}
            ]
          }
        }
      }
    },
    {
      "id": 5,
      "title": "DB Read Throughput",
      "type": "stat",
      "gridPos": {"h": 8, "w": 6, "x": 18, "y": 0},
      "targets": [
        {
          "expr": "rate(npgsql_db_client_commands_bytes_read[$__rate_interval])",
          "legendFormat": "Read Bytes/sec",
          "refId": "A"
        }
      ],
      "fieldConfig": {"defaults": {"unit": "Bps", "color": {"mode": "palette-classic"}}}
    },
    {
      "id": 5,
      "title": "DB Write Throughput",
      "type": "stat",
      "gridPos": {"h": 8, "w": 6, "x": 18, "y": 6},
      "targets": [
        {
          "expr": "rate(npgsql_db_client_commands_bytes_written[$__rate_interval])",
          "legendFormat": "Write Bytes/sec",
          "refId": "A"
        }
      ],
      "fieldConfig": {"defaults": {"unit": "Bps", "color": {"mode": "palette-classic"}}}
    },
    {
      "id": 6,
      "title": "Request Duration Over Time (Quantiles)",
      "type": "timeseries",
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8},
      "targets": [
        {
          "expr": "histogram_quantile(0.50, sum(rate(http_request_duration_seconds_bucket[$__rate_interval])) by (le))",
          "legendFormat": "p50",
          "refId": "A"
        },
        {
          "expr": "histogram_quantile(0.75, sum(rate(http_request_duration_seconds_bucket[$__rate_interval])) by (le))",
          "legendFormat": "p75",
          "refId": "B"
        },
        {
          "expr": "histogram_quantile(0.90, sum(rate(http_request_duration_seconds_bucket[$__rate_interval])) by (le))",
          "legendFormat": "p90",
          "refId": "C"
        },
        {
          "expr": "histogram_quantile(0.95, sum(rate(http_request_duration_seconds_bucket[$__rate_interval])) by (le))",
          "legendFormat": "p95",
          "refId": "D"
        },
        {
          "expr": "histogram_quantile(0.99, sum(rate(http_request_duration_seconds_bucket[$__rate_interval])) by (le))",
          "legendFormat": "p99",
          "refId": "E"
        }
      ],
      "fieldConfig": {"defaults": {"unit": "s", "color": {"mode": "palette-classic"}}}
    },
    {
      "fieldConfig": {
        "defaults": {
          "custom": {
            "hideFrom": {
              "legend": false,
              "tooltip": false,
              "viz": false
            },
            "scaleDistribution": {
              "type": "linear"
            }
          }
        },
        "overrides": []
      },
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8},
      "id": 7,
      "options": {
        "calculate": false,
        "cellGap": 1,
        "color": {
          "exponent": 0.5,
          "fill": "dark-orange",
          "mode": "scheme",
          "reverse": false,
          "scale": "exponential",
          "scheme": "RdYlBu",
          "steps": 64
        },
        "exemplars": {
          "color": "rgba(255,0,255,0.7)"
        },
        "filterValues": {
          "le": 1e-9
        },
        "legend": {
          "show": true
        },
        "rowsFrame": {
          "layout": "auto"
        },
        "tooltip": {
          "mode": "single",
          "showColorScale": false,
          "yHistogram": false
        },
        "yAxis": {
          "axisPlacement": "left",
          "reverse": false
        }
      },
      "pluginVersion": "11.3.1",
      "targets": [
        {
          "adhocFilters": [],
          "datasource": {
            "type": "prometheus",
            "uid": "PBFA97CFB590B2093"
          },
          "expr": "sum by(le) (rate(http_request_duration_seconds_bucket{}[$__rate_interval]))",
          "format": "heatmap",
          "interval": "",
          "refId": "Heatmap"
        }
      ],
      "title": "Request Duration Heatmap",
      "type": "heatmap"
    },
{
  "id": 8,
  "title": "Active Virtual Users (k6_vus)",
  "type": "timeseries",
  "gridPos": {"h": 8, "w": 12, "x": 0, "y": 16},
  "targets": [
    {
      "expr": "k6_vus",
      "legendFormat": "Virtual Users",
      "refId": "A"
    }
  ],
  "fieldConfig": {
    "defaults": {
      "unit": "short",
      "color": {"mode": "palette-classic"},
      "thresholds": {
        "mode": "absolute",
        "steps": [
          {"color": "green", "value": null},
          {"color": "orange", "value": 50},
          {"color": "red", "value": 100}
        ]
      }
    }
  },
  "options": {
    "showLegend": true,
    "legend": {
      "displayMode": "table",
      "placement": "bottom"
    },
    "tooltip": {
      "mode": "single"
    }
  }
},
    {
      "id": 9,
      "title": "Requests Per Second (RPS)",
      "type": "timeseries",
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 16},
      "targets": [
        {
          "expr": "sum by(controller) (sum by(action) (rate(http_requests_received_total[$__rate_interval])))",
          "legendFormat": "RPS",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "reqps",
          "color": {"mode": "palette-classic"}
        }
      }
    }
  ]
}
